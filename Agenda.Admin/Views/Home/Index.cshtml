    <script src="/bower_components/angular-viacep/dist/angular-viacep.min.js"></script>
    <script src="/bower_components/angular-ui-tinymce/src/tinymce.js"></script>
    <script src="/bower_components/ng-img-crop/compile/unminified/ng-img-crop.js"></script>
    <script src="/bower_components/jquery.nicescroll/jquery.nicescroll.min.js"></script>
    <script src="/bower_components/angular-nicescroll/angular-nicescroll.js"></script>




    <script src="/App/src/App.js"></script>

    <script src="/App/src/Services/Funcionarios/Agenda.js"></script>
    <script src="/App/src/Services/Login/Operadores.js"></script>

