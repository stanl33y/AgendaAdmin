    <script src="/App/src/Modules/Shared/Home/Route.js"></script>

    <script src="/App/src/Modules/Shared/Menu/Controller.js"></script>




    <!-- endinject -->
    <script>
        (function () {
            var docElem = document.documentElement,
                didScroll = false,
                changeHeaderOn = 100,
                currentPosition = 0;
            function init() {
                window.addEventListener('scroll', function (event) {
                    if (!didScroll) { didScroll = true; setTimeout(scrollPage, 250); }
                }, false);
            }
            function scrollPage() {
                var sy = window.pageYOffset || docElem.scrollTop;
                if (sy >= changeHeaderOn && sy <= currentPosition) {
                    jQuery(".navbar").addClass('navbar-fixed-top');
                    jQuery("body").addClass('navbar-margin');
                }
                else {
                    jQuery(".navbar").removeClass('navbar-fixed-top');
                    jQuery("body").removeClass('navbar-margin');
                }
                didScroll = false;
                currentPosition = sy;
            }
            init();
        })();
    </script>

</body>
</html>